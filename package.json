  "private": true,
  "scripts": {
    "build": "pnpm next build",
    "cf-typegen": "pnpm wrangler types --env-interface CloudflareEnv env.d.ts",
    "check": "pnpm run build && pnpm tsc",
    "check-pnpm": "node scripts/check-pnpm.js",
    "deploy": "pnpm opennextjs-cloudflare build && pnpm opennextjs-cloudflare deploy",
    "dev": "pnpm next dev",
    "drizzle": "drizzle-kit",
    "db:generate": "drizzle-kit generate",
    "db:pull": "drizzle-kit pull",
    "auth:generate": "npx @better-auth/cli generate -y && node scripts/transform-auth-schema.js",
    "auth:migrate": "npx @better-auth/cli migrate -y",
    "migrate:auth": "pnpm auth:migrate",
    "fix-schema": "node scripts/fix-schema-imports.js",
    "migrate:drizzle": "pnpm fix-schema && pnpm db:generate",
    "migrate:all": "pnpm migrate:auth && pnpm migrate:drizzle",
    "lint": "pnpm next lint",
    "preview": "pnpm opennextjs-cloudflare build && pnpm opennextjs-cloudflare preview",
    "start": "pnpm next start",
    "test": "pnpm jest",
    "test:watch": "pnpm jest --watch",
    "test:coverage": "pnpm jest --coverage",
    "tsc": "pnpm tsc --noEmit",
    "typecheck": "pnpm tsc --noEmit",
    "auth:fix": "node scripts/fix-auth-schema.js",
    "auth:setup": "pnpm auth:generate && pnpm auth:fix",
    "db:migrate": "pnpm drizzle-kit migrate",
    "db:push": "pnpm drizzle-kit push",
    "db:studio": "pnpm drizzle-kit studio",
    "setup:auth": "pnpm auth:setup && pnpm db:generate && pnpm db:migrate"
  }
}