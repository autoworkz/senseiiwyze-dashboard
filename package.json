  "private": true,
  "scripts": {
    "build": "pnpm next build",
    "clean": "rm -rf .next dist node_modules/.cache coverage .turbo",
    "check": "pnpm run build && pnpm tsc",
    "check-pnpm": "node scripts/check-pnpm.js",
    "deploy": "pnpm build",
    "dev": "pnpm next dev --turbo",
    "dev:webpack": "pnpm next dev",
    "lint": "pnpm next lint",
    "preview": "pnpm build && pnpm start",
    "start": "pnpm next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui --open",
    "test:debug": "VITEST_DEBUG=1 vitest --no-coverage --reporter=verbose",
    "test:auth": "vitest src/services/__tests__/authService.test.ts",
    "test:config": "vitest src/__tests__/config/",
    "test:components": "vitest src/components/",
    "test:parallel": "vitest --reporter=verbose --threads",
    "test:ci": "vitest run --coverage --reporter=verbose --reporter=junit",
    "typecheck": "tsc --noEmit",
    "test:e2e": "pnpm exec playwright test",
    "analyze": "pnpm build && open dist/stats.html",
    "analyze:size": "rollup-plugin-visualizer --template=treemap --open",
    "analyze:network": "rollup-plugin-visualizer --template=network --open",
    "pwa:dev": "vite --mode=development",
    "pwa:build": "vite build && workbox generateSW",
    "lint:watch": "vite --mode=development --watch",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit introspect",
    "db:studio": "drizzle-kit studio",
    "db:demo-users": "pnpm tsx scripts/setup-demo-users.ts",
    "test:api-complete": "pnpm tsx scripts/test-api-complete.ts",
    "test:critical": "pnpm tsx scripts/test-api-complete.ts --critical",
    "test:high": "pnpm tsx scripts/test-api-complete.ts --high",
    "test:auth-basic": "pnpm tsx scripts/test-better-auth-api.ts",
    "test:auth-flows": "pnpm tsx scripts/test-auth-flows.ts",
    "test:user-management": "pnpm tsx scripts/test-user-management.ts",
    "test:oauth": "pnpm tsx scripts/test-oauth-providers.ts",
    "test:2fa": "pnpm tsx scripts/test-2fa-plugin.ts",
    "test:database": "pnpm tsx scripts/verify-db-connection.ts",
    "auth:generate": "bunx @better-auth/cli generate",
    "auth:migrate": "bunx @better-auth/cli generate",
    "dev:watch-simple": "bash scripts/dev.sh simple",
    "dev:watch-robust": "bash scripts/dev.sh robust",
    "dev:comprehensive": "bash scripts/dev-comprehensive.sh",
    "dev:clean": "bash scripts/dev-comprehensive.sh clean",
    "dev:fast": "bash scripts/dev-comprehensive.sh fast",
